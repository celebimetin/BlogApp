@model HomeBlogModel
@{
    ViewData["Title"] = "Home Index";
}

<div class="container mt-5">
    <br />
    <div class="row mb-2">
        <div class="col-md-3">
            @await Component.InvokeAsync("CategoryMenu")
        </div>
        <div class="col-md-9">
            @Html.Partial("_Slider", Model.SliderBlogs)
            @Html.Partial("_BlogList", Model.HomeBlogs)
        </div>
    </div>
</div>